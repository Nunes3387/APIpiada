public class Tipo {
    private String id;
    private String descricao;
    
    // construtor vazio
    public Tipo() {}
    
    // construtor com par√¢metros
    public Tipo(String id, String descricao) {
        this.id = id;
        this.descricao = descricao;
    }
    
    // getters e setters
    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getDescricao() {
        return descricao;
    }

    public void setDescricao(String descricao) {
        this.descricao = descricao;
    }
}
