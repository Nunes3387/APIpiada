public class Piada {
    private String id;
    private Tipo tipo;
    private String descricao;
    private String titulo;
    
    // construtor vazio
    public Piada() {}
    
    // construtor com par√¢metros
    public Piada(String id, Tipo tipo, String descricao, String titulo) {
        this.id = id;
        this.tipo = tipo;
        this.descricao = descricao;
        this.titulo = titulo;
    }
    
    // getters e setters
    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public Tipo getTipo() {
        return tipo;
    }

    public void setTipo(Tipo tipo) {
        this.tipo = tipo;
    }

    public String getDescricao() {
        return descricao;
    }

    public void setDescricao(String descricao) {
        this.descricao = descricao;
    }

    public String getTitulo() {
        return titulo;
    }

    public void setTitulo(String titulo) {
        this.titulo = titulo;
    }
}
